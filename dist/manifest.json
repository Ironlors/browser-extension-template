{
	"name": "Unix Timeconverter",
	"version": "0.1",
	"description": "A tool for converting Unix Timestamp into other data formats",
	"manifest_version": 2,
	"permissions": ["clipboardRead", "clipboardWrite"],
	"browser_action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "res/img/icon16.png",
		"48": "res/img/icon48.png",
		"128": "res/img/icon128.png"
	}
}
